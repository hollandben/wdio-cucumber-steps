{
    "collectCoverage": true,
    "coverageDirectory": "./coverage",
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/support/**/*.js",
      "src/given/**/*.js",
      "src/when/**/*.js",
      "src/then/**/*.js"
    ],
    "notify": true,
    "setupTestFrameworkScriptFile": "<rootDir>/test/setup.js",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testURL": "http://localhost"
  }
